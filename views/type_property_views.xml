<?xml version="1.0"?>
<odoo>
    <record id="type_model_action" model="ir.actions.act_window">
        <field name="name">Types of entries</field>
        <field name="res_model">type_model</field>
        <field name="view_mode">tree,form</field>
    </record>

    <record id="type_model_tree_view" model="ir.ui.view">
        <field name="name">type_model.tree</field>
        <field name="model">type_model</field>
        <field name="arch" type="xml">
            <tree editable="bottom">
                <field name="sequence" widget="handle"/>
                <field name="name"/>
            </tree>
        </field>
    </record>

    <record id="type_model_view" model="ir.ui.view">
        <field name="name">type_model.form</field>
        <field name="model">type_model</field>
        <field name="arch" type="xml">
            <form string="test">
                <sheet>
                    <h1>
                        <field name="name"/>
                    </h1>
                </sheet>
            </form>
        </field>
    </record>
</odoo>